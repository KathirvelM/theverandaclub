    <!-- Base application theme. -->
    <style name="AppTheme" parent="AppTheme.Base">
        <!-- <item name="android:homeAsUpIndicator">@drawable/ic_action_back</item>-->
        <!-- Customize your theme here. -->
        <item name="colorControlNormal">@color/colorControlNormal</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorControlHighlight">@color/colorAccent</item>
        <item name="fontFamily">@font/roboto_light</item>

        <item name="tv">@color/tv</item>
        <item name="tvDisplay">@color/tvDisplay</item>
        <item name="tvHead">@color/tvHead</item>
        <item name="tvHeadSub">@color/tvHeadSub</item>
        <item name="tvTitle">@color/tvTitle</item>
        <item name="tvTitleSub">@color/tvTitleSub</item>
        <item name="tvToolbar">@color/tvToolBar</item>
        <item name="et">@color/et</item>
        <item name="etHint">@color/etHint</item>
        <item name="etBg">@color/etBgTint</item>
        <item name="btn">@color/tvBtn</item>
        <item name="btnTrans">@color/tv</item>

        <item name="divider">@color/divider</item>
        <item name="btnBg">@drawable/bg_button</item>

        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
    </style>


