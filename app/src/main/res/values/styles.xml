        <item name="tvHead">@color/_tvHead</item>
        <item name="tvHeadSub">@color/_tvHeadSub</item>
        <item name="tvTitle">@color/_tvTitle</item>
        <item name="tvTitleSub">@color/_tvTitleSub</item>
        <item name="tvToolbar">@color/_tvToolBar</item>
